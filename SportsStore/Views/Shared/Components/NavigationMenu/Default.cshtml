@model IEnumerable<string>

<a class="btn btn-block btn-default"
   asp-action="List"
   asp-controller="Product"
   asp-route-category="">All</a>

@foreach (string item in Model)
{
    <a class="btn btn-block @(@item == ViewBag.SelectedCategory ? "btn-primary" : "btn-default")"
       asp-action="List"
       asp-controller="Product"
       asp-route-category="@item"
       asp-route-page="1">
        @item
    </a>
}